<block>
    <block wx:if="{{!list}}">
        <loading __triggerObserer="{{ _triggerObserer }}"></loading>
    </block>
    <view class="page-category-home" wx:else>
        <nav-bar title="分类" __triggerObserer="{{ _triggerObserer }}" leftIconType="chevron-left" fixed="true"></nav-bar>
        <search-bar className="category-top" __triggerObserer="{{ _triggerObserer }}" params="{{info}}" isFixed="{{true}}"></search-bar>
        <scroll-view class="category-list__content" scroll-y="{{true}}">
            <view class="{{anonymousState__temp}}">
                <view class="category-content__img" wx:key="$original.category_id" bindtap="handleClickItem" wx:for="{{loopArray0}}" wx:for-item="item" wx:for-index="_anonIdx" data-e-tap-so="this" data-e-tap-a-a="{{item.$original}}">
                    <image class="{{item.$loopState__temp3}}" mode="aspectFill" src="{{item.$original.image_url}}"></image>
                    <view class="img-cat-name">{{item.$original.category_name}}</view>
                </view>
                <view class="category-content__img-empty"></view>
                <view class="category-content__img-empty"></view>
                <view class="category-content__img-empty"></view>
                <view class="category-content__img-empty"></view>
                <view class="category-content__img-empty"></view>
            </view>
        </scroll-view>
    </view>
</block>